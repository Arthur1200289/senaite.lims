  <genericsetup:upgradeStep
      title="Upgrade SENAITE LIMS"
      description="Version 2.3.0"
      source="2200"
      destination="2300"
      handler="senaite.lims.upgrades.handlers.to_2300"
      profile="senaite.lims:default" />

  <genericsetup:upgradeStep
      title="Upgrade SENAITE LIMS"
